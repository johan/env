[alias]
        b = branch
        bb = branch -avv
        bd = branch -d
        bnuke = !git branch -d $1 && git push --delete origin
        c = commit -am
        ce = commit -ae
        co = checkout
        d = diff
        df = diff --name-status
        f = fetch
        l = log
        lt = log --oneline --graph
        lf = log --name-status
        p = pull
        pp = push -u origin HEAD
        s = status -sb
        setuser = ! git_setuser.py
[color]
        diff = auto
        status = auto
        branch = auto
        interactive = auto
        ui = auto
        pager = true
